{
    "extends": [
        "airbnb",
        "plugin:prettier/recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "plugin:jsx-a11y/recommended",
        "prettier"
    ],
    "plugins": ["prettier", "react"],
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "react/no-unescaped-entities": "off",
        "react/prop-types": 0,
        "import/extensions": ["error", "ignorePackages"],
        "no-console": "off",
        "semi": ["error", "always"]
    }
}
